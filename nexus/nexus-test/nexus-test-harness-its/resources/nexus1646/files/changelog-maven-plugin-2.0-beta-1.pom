    <mailingList>
      <name>Commits List</name>
      <subscribe>scm-subscribe@mojo.codehaus.org</subscribe>
      <unsubscribe>scm-unsubscribe@mojo.codehaus.org</unsubscribe>
      <archive>http://archive.mojo.codehaus.org/scm/</archive>
