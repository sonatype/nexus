<?xml version="1.0" encoding="UTF-8"?>
<nexusConfiguration>
	<version>1.0</version>
	<workingDirectory>/dat/maven/mvnrepos</workingDirectory>
        <applicationLogDirectory>${basedir}/logs/</applicationLogDirectory> 
	<security>
		<enabled>false</enabled>
	
	</security>
	<!-- Global connection settings
		<globalConnectionSettings>
		<connectionTimeout>10000</connectionTimeout>
		<retrievalRetryCount>3</retrievalRetryCount>
		<queryString></queryString>
		<userAgentString>Nexus/1.0</userAgentString>
		</globalConnectionSettings>
	-->
	<!-- Global HTTP Proxy settings-->
		<globalHttpProxySettings>
		<proxyHostname>proxyhost</proxyHostname>
		<proxyPort>8080</proxyPort>
		<authentication>
		<username></username>
		<password></password>
		</authentication>
		</globalHttpProxySettings>
	<routing>
		<repositories>
			<followLinks>true</followLinks>
			<notFoundCacheTTL>1440</notFoundCacheTTL>
		</repositories>
		<groups>
			<followLinks>true</followLinks>
			<notFoundCacheTTL>1440</notFoundCacheTTL>
			<stopItemSearchOnFirstFoundFile>
				true
			</stopItemSearchOnFirstFoundFile>
			<mergeMetadata>true</mergeMetadata>
			<pathMapping>
				<inclusions>
					<inclusion>
						<key>.*/org/apache/.*</key>
						<value>central,sonatype,apache.snapshot,apache.incubator</value>
					</inclusion>
                                        <inclusion>
                                                <key>.*/org/codehaus/.*</key>
                                                <value>central,codehaus.snapshot,sonatype</value>
                                        </inclusion>
				</inclusions>
				<exclusions>
                                        <!--exclude dec artifacts from ALL external repositories-->
					<exclusion>
						<key>.*/com/dec/.*</key>
						<value>sonatype,apache.incubator,central,freehep,atlassian,apache.snapshot,codehaus.snapshot,apache.snapshot,freehep.snapshot,atlassian.snapshot</value>
					</exclusion>
				</exclusions>
			</pathMapping>
		</groups>
	</routing>

	<repositories>

		<!-- PUBLIC REPOSITORIES -->
		<repository>
			<id>central</id>
			<name>Maven Central</name>
			<available>true</available>
			<readOnly>true</readOnly>
			<browseable>true</browseable>
			<offline>false</offline>
			<indexable>true</indexable>
			<notFoundCacheTTL>86400</notFoundCacheTTL>
			<itemMaxAge>86400</itemMaxAge>
			<shouldServeReleases>true</shouldServeReleases>
			<shouldServeSnapshots>false</shouldServeSnapshots>
			<remoteStorage>
				<url>http://repo1.maven.org/maven2/</url>
			</remoteStorage>
		</repository>
                <repository>
                        <id>sonatype</id>
                        <name>Sonatype</name>
                        <available>true</available>
                        <readOnly>true</readOnly>
                        <browseable>true</browseable>
                        <offline>false</offline>
                        <indexable>true</indexable>
                        <notFoundCacheTTL>186400</notFoundCacheTTL>
                        <itemMaxAge>86400</itemMaxAge>
                        <shouldServeReleases>true</shouldServeReleases>
                        <shouldServeSnapshots>false</shouldServeSnapshots>
                        <remoteStorage>
                                <url>http://hehe.sonatype.com/repos/customer</url>
                        </remoteStorage>
                </repository>	
                <repository>
			<id>freehep</id>
			<name>Freehep</name>
			<available>true</available>
			<readOnly>true</readOnly>
			<browseable>true</browseable>
			<offline>false</offline>
			<indexable>true</indexable>
			<notFoundCacheTTL>86400</notFoundCacheTTL>
			<itemMaxAge>86400</itemMaxAge>
			<shouldServeReleases>true</shouldServeReleases>
			<shouldServeSnapshots>false</shouldServeSnapshots>
			<remoteStorage>
				<url>http://java.freehep.org/maven2/</url>
			</remoteStorage>
		</repository>
                <repository>
                        <id>apache.incubator</id>
                        <name>Apache Inbubator</name>
                        <available>true</available>
                        <readOnly>true</readOnly>
                        <browseable>true</browseable>
                        <offline>false</offline>
                        <indexable>true</indexable>
                        <notFoundCacheTTL>86400</notFoundCacheTTL>
                        <itemMaxAge>86400</itemMaxAge>
                        <shouldServeReleases>true</shouldServeReleases>
                        <shouldServeSnapshots>false</shouldServeSnapshots>
                        <remoteStorage>
                                <url>http://people.apache.org/repo/m2-incubating-repository/</url>
                        </remoteStorage>
                </repository>
		<repository>
			<id>atlassian</id>
			<name>Atlassian</name>
			<available>true</available>
			<readOnly>true</readOnly>
			<browseable>true</browseable>
			<offline>false</offline>
			<indexable>true</indexable>
			<notFoundCacheTTL>86400</notFoundCacheTTL>
			<itemMaxAge>86400</itemMaxAge>
			<shouldServeReleases>true</shouldServeReleases>
			<shouldServeSnapshots>false</shouldServeSnapshots>
			<remoteStorage>
				<url>https://maven.atlassian.com/public/</url>
			</remoteStorage>
		</repository>
                <repository>
                        <id>openfast</id>
                        <name>OpenFAST</name>
                        <available>true</available>
                        <readOnly>true</readOnly>
                        <browseable>true</browseable>
                        <offline>false</offline>
                        <indexable>true</indexable>
                        <notFoundCacheTTL>86400</notFoundCacheTTL>
                        <itemMaxAge>86400</itemMaxAge>
                        <shouldServeReleases>true</shouldServeReleases>
                        <shouldServeSnapshots>false</shouldServeSnapshots>
                        <remoteStorage>
                         <url>http://www.openfast.org/maven/release/</url>
                        </remoteStorage>
                </repository>
		<repository>
			<id>codehaus.snapshot</id>
			<name>Codehaus Snapshots</name>
			<available>true</available>
			<readOnly>true</readOnly>
			<browseable>true</browseable>
			<offline>false</offline>
			<indexable>true</indexable>
			<notFoundCacheTTL>86400</notFoundCacheTTL>
			<itemMaxAge>86400</itemMaxAge>
			<shouldServeReleases>false</shouldServeReleases>
			<shouldServeSnapshots>true</shouldServeSnapshots>
			<remoteStorage>
				<url>http://snapshots.repository.codehaus.org/</url>
			</remoteStorage>
		</repository>
    <repository>
        <id>apache.snapshot</id>
        <name>Apache Snapshots</name>
        <available>true</available>
        <readOnly>true</readOnly>
        <browseable>true</browseable>
        <offline>false</offline>
        <indexable>true</indexable>
        <notFoundCacheTTL>6400</notFoundCacheTTL>
        <itemMaxAge>60</itemMaxAge>
        <shouldServeReleases>false</shouldServeReleases>
        <shouldServeSnapshots>true</shouldServeSnapshots>
        <remoteStorage>
                <url>http://people.apache.org/repo/m2-snapshot-repository/</url>
        </remoteStorage>
    </repository>
    <repository>
        <id>freehep.snapshot</id>
        <name>Freehep Snapshots</name>
        <available>true</available>
        <readOnly>true</readOnly>
        <browseable>true</browseable>
        <offline>false</offline>
        <indexable>true</indexable>
        <notFoundCacheTTL>86400</notFoundCacheTTL>
        <itemMaxAge>86400</itemMaxAge>
        <shouldServeReleases>false</shouldServeReleases>
        <shouldServeSnapshots>true</shouldServeSnapshots>
        <remoteStorage>
                <url>http://java.freehep.org/maven2/</url>
        </remoteStorage>
		</repository>
     <repository>
        <id>atlassian.snapshot</id>
        <name>Atlassian Snapshots</name>
        <available>true</available>
        <readOnly>true</readOnly>
        <browseable>true</browseable>
        <offline>false</offline>
        <indexable>true</indexable>
        <notFoundCacheTTL>86400</notFoundCacheTTL>
        <itemMaxAge>86400</itemMaxAge>
        <shouldServeReleases>false</shouldServeReleases>
        <shouldServeSnapshots>true</shouldServeSnapshots>
        <remoteStorage>
                <url>https://maven.atlassian.com/public-snapshot/</url>
        </remoteStorage>
      </repository>
    <repository>
        <id>openfast.snapshot</id>
        <name>Openfast Snapshots</name>
        <available>true</available>
        <readOnly>true</readOnly>
        <browseable>true</browseable>
        <offline>false</offline>
        <indexable>true</indexable>
        <notFoundCacheTTL>86400</notFoundCacheTTL>
        <itemMaxAge>86400</itemMaxAge>
        <shouldServeReleases>false</shouldServeReleases>
        <shouldServeSnapshots>true</shouldServeSnapshots>
        <remoteStorage>
          <url>http://www.openfast.org/maven/snapshot/</url>
        </remoteStorage>
      </repository>
	</repositories>
	
	<repositoryGroups>
		<repositoryGroup>
			<groupId>public</groupId>
			<repositories>
				<repository>central</repository>
                                <repository>sonatype</repository>
				<repository>freehep</repository>
				<repository>atlassian</repository>
                                <repository>apache.incubator</repository>
                                <repository>openfast</repository>
			</repositories>
		</repositoryGroup>
		<repositoryGroup>
			<groupId>snapshot</groupId>
			<repositories>
				<repository>apache.snapshot</repository>
				<repository>codehaus.snapshot</repository>
				<repository>freehep.snapshot</repository>
				<repository>atlassian.snapshot</repository>
				<repository>openfast.snapshot</repository>
			</repositories>
		</repositoryGroup>
	</repositoryGroups>

</nexusConfiguration>
